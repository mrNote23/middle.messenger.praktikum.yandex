{
  "compilerOptions":{
    "outDir":"dist",
    "target":"es2017",
    "esModuleInterop":true,
    "forceConsistentCasingInFileNames":true,
    "skipLibCheck":true,
    "declaration":false,
    "module":"commonjs",
    "noImplicitAny":false,
    "noUnusedLocals":true,
    "noUnusedParameters":true,
    "sourceMap":true,
    "strictNullChecks":false,
    "isolatedModules":true
  },
  "include":[
    "./src/**/*.ts"
  ],
  "exclude":[
    "node_modules",
    "dist"
  ]
}
