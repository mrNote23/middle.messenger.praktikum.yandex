{
  "extends":[
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser":"@typescript-eslint/parser",
  "plugins":[
    "@typescript-eslint"
  ],
  "root":true,
  "env":{
    "browser":true
  },
  "rules":{
    "@typescript-eslint/no-explicit-any": ["off"],
    "max-params":[
      2,
      4
    ],
    "quotes":[
      "error",
      "double",
      {
        "avoidEscape":true,
        "allowTemplateLiterals":true
      }
    ]
  }
}
